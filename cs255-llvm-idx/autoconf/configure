	[lt_dict_add_subkey([lt_decl_dict], [lt_cv_sys_global_symbol_pipe], [description], [Take the output of nm and produce a listing of raw symbols and C names])])
    lt_dict_add_subkey([lt_decl_dict], [lt_cv_sys_global_symbol_pipe],
	[tagged?], [m4_ifval([], [yes], [no])]))

lt_if_append_uniq(lt_decl_varnames, lt_cv_sys_global_symbol_to_cdecl, , ,
    lt_dict_add_subkey([lt_decl_dict], [lt_cv_sys_global_symbol_to_cdecl], [libtool_name],
