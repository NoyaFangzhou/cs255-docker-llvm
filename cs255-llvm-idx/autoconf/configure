else

$as_echo "#define error_t int" >>confdefs.h


$as_echo "#define __error_t_defined 1" >>confdefs.h

fi


ARGZ_H=
for ac_func in argz_add argz_append argz_count argz_create_sep argz_insert \
	argz_next argz_stringify
