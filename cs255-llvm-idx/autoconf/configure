    lt_dict_add_subkey([lt_decl_dict], [objdir], [libtool_name],
	[m4_ifval([], [], [objdir])])
    lt_dict_add_subkey([lt_decl_dict], [objdir], [value], [0])
    m4_ifval([The name of the directory that contains temporary libtool files],
	[lt_dict_add_subkey([lt_decl_dict], [objdir], [description], [The name of the directory that contains temporary libtool files])])
    lt_dict_add_subkey([lt_decl_dict], [objdir],
	[tagged?], [m4_ifval([], [yes], [no])]))

