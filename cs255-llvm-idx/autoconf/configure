    lt_dict_add_subkey([lt_decl_dict], [lt_cv_sys_global_symbol_to_c_name_address], [value], [1])
    m4_ifval([Transform the output of nm in a C name address pair],
	[lt_dict_add_subkey([lt_decl_dict], [lt_cv_sys_global_symbol_to_c_name_address], [description], [Transform the output of nm in a C name address pair])])
    lt_dict_add_subkey([lt_decl_dict], [lt_cv_sys_global_symbol_to_c_name_address],
	[tagged?], [m4_ifval([], [yes], [no])]))

lt_if_append_uniq(lt_decl_varnames, lt_cv_sys_global_symbol_to_c_name_address_lib_prefix, , ,
    lt_dict_add_subkey([lt_decl_dict], [lt_cv_sys_global_symbol_to_c_name_address_lib_prefix], [libtool_name],
	[m4_ifval([global_symbol_to_c_name_address_lib_prefix], [global_symbol_to_c_name_address_lib_prefix], [lt_cv_sys_global_symbol_to_c_name_address_lib_prefix])])
    lt_dict_add_subkey([lt_decl_dict], [lt_cv_sys_global_symbol_to_c_name_address_lib_prefix], [value], [1])
    m4_ifval([Transform the output of nm in a C name address pair when lib prefix is needed],
	[lt_dict_add_subkey([lt_decl_dict], [lt_cv_sys_global_symbol_to_c_name_address_lib_prefix], [description], [Transform the output of nm in a C name address pair when lib prefix is needed])])
    lt_dict_add_subkey([lt_decl_dict], [lt_cv_sys_global_symbol_to_c_name_address_lib_prefix],
	[tagged?], [m4_ifval([], [yes], [no])]))

lt_if_append_uniq(lt_decl_varnames, nm_file_list_spec, , ,
    lt_dict_add_subkey([lt_decl_dict], [nm_file_list_spec], [libtool_name],
	[m4_ifval([], [], [nm_file_list_spec])])
    lt_dict_add_subkey([lt_decl_dict], [nm_file_list_spec], [value], [1])
    m4_ifval([Specify filename containing input files for $NM],
	[lt_dict_add_subkey([lt_decl_dict], [nm_file_list_spec], [description], [Specify filename containing input files for $NM])])
    lt_dict_add_subkey([lt_decl_dict], [nm_file_list_spec],
	[tagged?], [m4_ifval([], [yes], [no])]))


{ $as_echo "$as_me:${as_lineno-$LINENO}: checking for _ prefix in compiled symbols" >&5
$as_echo_n "checking for _ prefix in compiled symbols... " >&6; }
if ${lt_cv_sys_symbol_underscore+:} false; then :
  $as_echo_n "(cached) " >&6
else
  lt_cv_sys_symbol_underscore=no
  cat > conftest.$ac_ext <<_LT_EOF
void nm_test_func(){}
int main(){nm_test_func;return 0;}
_LT_EOF
  if { { eval echo "\"\$as_me\":${as_lineno-$LINENO}: \"$ac_compile\""; } >&5
  (eval $ac_compile) 2>&5
  ac_status=$?
  $as_echo "$as_me:${as_lineno-$LINENO}: \$? = $ac_status" >&5
  test $ac_status = 0; }; then
    # Now try to grab the symbols.
    ac_nlist=conftest.nm
    if { { eval echo "\"\$as_me\":${as_lineno-$LINENO}: \"$NM conftest.$ac_objext \| $lt_cv_sys_global_symbol_pipe \> $ac_nlist\""; } >&5
  (eval $NM conftest.$ac_objext \| $lt_cv_sys_global_symbol_pipe \> $ac_nlist) 2>&5
  ac_status=$?
  $as_echo "$as_me:${as_lineno-$LINENO}: \$? = $ac_status" >&5
  test $ac_status = 0; } && test -s "$ac_nlist"; then
      # See whether the symbols have a leading underscore.
      if grep '^. _nm_test_func' "$ac_nlist" >/dev/null; then
        lt_cv_sys_symbol_underscore=yes
      else
        if grep '^. nm_test_func ' "$ac_nlist" >/dev/null; then
	  :
        else
	  echo "configure: cannot find nm_test_func in $ac_nlist" >&5
        fi
      fi
    else
      echo "configure: cannot run $lt_cv_sys_global_symbol_pipe" >&5
    fi
  else
    echo "configure: failed program was:" >&5
    cat conftest.c >&5
  fi
  rm -rf conftest*

fi
{ $as_echo "$as_me:${as_lineno-$LINENO}: result: $lt_cv_sys_symbol_underscore" >&5
$as_echo "$lt_cv_sys_symbol_underscore" >&6; }
  sys_symbol_underscore=$lt_cv_sys_symbol_underscore


for ac_header in dlfcn.h
do :
  ac_fn_c_check_header_compile "$LINENO" "dlfcn.h" "ac_cv_header_dlfcn_h" "$ac_includes_default
"
if test "x$ac_cv_header_dlfcn_h" = xyes; then :
  cat >>confdefs.h <<_ACEOF
