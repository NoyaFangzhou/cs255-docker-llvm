  nm_file_list_spec='@'
elif $NM --help 2>/dev/null | grep '[@]FILE' >/dev/null; then
  nm_file_list_spec='@'
fi

lt_if_append_uniq(lt_decl_varnames, lt_cv_sys_global_symbol_pipe, , ,
    lt_dict_add_subkey([lt_decl_dict], [lt_cv_sys_global_symbol_pipe], [libtool_name],
